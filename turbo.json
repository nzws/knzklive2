{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["**/*.tsx", "**/*.ts", "**/*.ts", "**/*.tsx"]
    },
    "lint": {
      "outputs": []
    }
  }
}
